<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="ConfigureTaxApp">
        <property name="MessageAttributes" dataBuilderRule="MessageAttribute[]"/>
        <property name="applicationId" dataBuilderRule="lexify('??????????')"/>
        <property name="apiUrl" dataBuilderRule="lexify('??????????')"/>
        <property name="apiUrls" dataBuilderRule="TaxAppApiUrls[]"/>
        <property name="vendorCode" dataBuilderRule="lexify('??????????')"/>
        <property name="storeReference" dataBuilderRule="=de-DE"/>
    </transfer>

    <transfer name="TaxAppApiUrls">
        <property name="quotationUrl" dataBuilderRule="lexify('??????????')"/>
        <property name="refundsUrl" dataBuilderRule="lexify('??????????')"/>
    </transfer>

    <transfer name="DeleteTaxApp">
        <property name="MessageAttributes" dataBuilderRule="MessageAttribute[]"/>
        <property name="vendorCode" dataBuilderRule="lexify('??????????')"/>
        <property name="storeReference" dataBuilderRule="=de-DE"/>
    </transfer>

    <transfer name="MessageAttribute">
        <property name="emitter" dataBuilderRule="word"/>
    </transfer>

</transfers>
