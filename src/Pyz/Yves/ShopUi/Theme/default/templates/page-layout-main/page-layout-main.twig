                                </g>
                                <defs>
                                    <clipPath id="clip0_438_9117">
                                        <rect width="65.9356" height="23.427" fill="white" transform="translate(31.0312 27.7861)"/>
                                    </clipPath>
                                </defs>
                            </svg>

                        </div>
                    </div>
                {% endblock %}

                {% block navigationInner %}
                    <div class="grid grid--gap">
                        {% cms_slot 'slt-footer-navigation' %}
                    </div>
                {% endblock %}

                {% block socialLinks %}
                    {% cms_slot 'slt-footer-social-links' %}
                {% endblock %}
            {% endembed %}
        {% endblock %}
    </div>

    {% include atom('touch-checker') only %}
{% endblock %}
